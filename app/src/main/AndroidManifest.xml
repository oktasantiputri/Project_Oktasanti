<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectokta">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectOkta"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".DetailMateriActivity"
            android:exported="false" />
        <activity
            android:name=".TambahMateriActivity"
            android:exported="false" />
        <activity
            android:name=".DetailInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".TambahInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".DetailPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".TambahPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>